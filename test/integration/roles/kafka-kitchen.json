{
  "name": "kafka-kitchen",
  "json_class": "Chef::Role",
  "default_attributes": {
    "confluent-platform": {
      "zookeeper": {
        "hosts": [
          "zookeeper-kafka.kitchen"
        ]
      },
      "kafka": {
        "role": "kafka-kitchen",
        "size": 3,
        "config": {
          "num.partitions": 3,
          "default.replication.factor": 3,
          "auto.create.topics.enable": "true"
        }
      }
    }
  },
  "override_attributes": {

  },
  "chef_type": "role",
  "run_list": [
    "recipe[confluent-platform::default]"
  ],
  "env_run_lists": {

  }
}
